{
    "name": "watchtower/watchtower-laravel",
    "description": "watchtower-laravel",
    "keywords": [
        "LaravelAgent",
        "laravel",
        "watchtower-laravel"
    ],
    "homepage": "https://github.com/watchtower/watchtower-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Candour Developers",
            "email": "devs@withcandour.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.48.2||^11.0||^12.0"
    },
    "autoload": {
        "psr-4": {
            "Watchtower\\WatchtowerLaravel\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Watchtower\\WatchtowerLaravel\\WatchtowerLaravelServiceProvider"
            ],
            "aliases": {
                "WatchtowerLaravel": "Watchtower\\WatchtowerLaravel\\Facades\\WatchtowerLaravel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}